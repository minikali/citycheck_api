{
  "connection": "default",
  "collectionName": "messages",
  "info": {
    "name": "Message"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "message": {
      "type": "text"
    },
    "file": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "message_id": {
      "type": "string"
    }
  }
}
