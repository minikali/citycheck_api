{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "text"
    },
    "title": {
      "type": "string"
    },
    "lng": {
      "type": "float"
    },
    "lat": {
      "type": "float"
    },
    "address": {
      "type": "string"
    },
    "phase": {
      "type": "integer"
    },
    "userConf": {
      "type": "component",
      "repeatable": true,
      "component": "confirm.project-confirm",
      "required": false
    },
    "userSuggest": {
      "type": "component",
      "repeatable": true,
      "component": "suggest.project-suggest"
    },
    "userinfo": {
      "model": "userinfo"
    },
    "valid": {
      "type": "boolean",
      "default": false,
      "required": false
    }
  }
}
