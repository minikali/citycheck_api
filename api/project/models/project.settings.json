{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "text"
    },
    "title": {
      "type": "string"
    },
    "lng": {
      "type": "float"
    },
    "lat": {
      "type": "float"
    },
    "address": {
      "type": "string"
    },
    "phase": {
      "type": "integer"
    },
    "userinfo": {
      "model": "userinfo"
    },
    "valid": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "justify": {
      "type": "string"
    },
    "project_suggestions": {
      "collection": "project-suggestion",
      "via": "project"
    },
    "project_histories": {
      "collection": "project-history",
      "via": "project"
    },
    "project_confirmations": {
      "collection": "project-confirmation",
      "via": "project"
    }
  }
}
