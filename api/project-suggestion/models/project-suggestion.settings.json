{
  "connection": "default",
  "collectionName": "project_suggestions",
  "info": {
    "name": "Project-Suggestion"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "phase": {
      "type": "integer"
    },
    "justify": {
      "type": "string"
    },
    "userinfo": {
      "model": "userinfo"
    },
    "valid": {
      "type": "boolean",
      "default": false
    },
    "french_project": {
      "model": "french-project",
      "via": "project_suggestions"
    },
    "english_project": {
      "model": "english-project",
      "via": "project_suggestions"
    },
    "justify_fr": {
      "type": "text"
    },
    "justify_en": {
      "type": "text"
    }
  }
}
