{
  "connection": "default",
  "collectionName": "project_histories",
  "info": {
    "name": "Project-History"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "justify": {
      "type": "string"
    },
    "phase": {
      "type": "integer"
    },
    "userinfo": {
      "model": "userinfo"
    },
    "name": {
      "type": "string"
    },
    "french_project": {
      "via": "project_histories",
      "model": "french-project"
    },
    "english_project": {
      "via": "project_histories",
      "model": "english-project"
    }
  }
}
